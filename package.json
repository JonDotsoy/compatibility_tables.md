{
  "name": "@jondotsoy/compatibility_table",
  "version": "0.1.1",
  "description": "Create a compatibility file for your project.",
  "module": "./lib/esm/index.js",
  "type": "module",
  "scripts": {
    "fmt": "prettier -w ."
  },
  "typesVersions": {
    "*": {
      "*": [
        "./lib/types/*"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./lib/types/index.d.ts",
      "default": "./lib/esm/index.js"
    }
  },
  "files": [
    "./docs/",
    "./lib/esm/",
    "./lib/types/"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/JonDotsoy/compatibility_tables.md.git"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/semver": "^7.5.8"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@jondotsoy/unindent": "^0.1.1",
    "glob": "^10.3.12",
    "mdast": "^3.0.0",
    "prettier": "^3.2.5",
    "remark-frontmatter": "^5.0.0",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "semver": "^7.6.0",
    "unified": "^11.0.4",
    "yaml": "^2.4.1",
    "zod": "^3.22.4"
  }
}
